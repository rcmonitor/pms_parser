<?xml version="1.0" ?>

<project name="pms_parser" basedir="." default="build">


	<property file="build.properties" />
	
	<condition property="all.modules.present">
		<and>
			<available file="pms_processor" type="dir" />
			<available file="pms_parser_emitter" type="dir" />
		</and>
	</condition>

	<target name="log_parameters">
		<echo file="parameters.log">
build.path = ${build.path}
test.parameter = ${test.parameter}
build.path.publisher = ${build.path.publisher}
build.path.emitter = ${build.path.emitter}
		</echo>
	</target>

	<target name="init">
		<echo message="${all.modules.present}" />
	</target>

	<target name="build" depends="init">
		<echo message="${build.path}" />
		<echo message="${test.parameter}" />
	</target>


</project>